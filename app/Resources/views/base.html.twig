{% block html_page %}
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta charset="utf-8" name="viewport" content="width=device-width, initial-scale=1.0">
	<meta charset="utf-8" name="keywords" content="LaFunete, Info, UNLP, Prestamos" />
	<meta charset="utf-8" name="description" content="" />
	<title>La Fuente :: Prestamos{% if page_title is defined %} :: {{ page_title }}{% endif %}</title>
	<link href="{{ asset('favicon.png') }}" rel="shortcut icon" type="image/png" />
	<link href="{{ asset('http://fonts.googleapis.com/css?family=Material+Icons|Roboto:300italic,400italic,300,400,500,700,900')}}" rel='stylesheet' type='text/css'>
  {# <link href="{{ asset('css/pre-compiled.min.css') }}" rel="stylesheet" type="text/css"> #}
	<link href="{{ asset('bower_components/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
	<link href="{{ asset('less/style.less') }}" rel="stylesheet/less">
	<link href="{{ asset('css/custom.css') }}" rel="stylesheet/less">
	{% block stylesheets %}{% endblock %}
	<script src="{{ asset('bower_components/less/dist/less.js') }}" type="text/javascript"></script>
</head>
<body class="nano menu-hidden">
	<div class="nano-content">

  <header>
  	{% include 'navbar.html.twig' %}
  </header><!-- /header -->

  <aside id="mainMenu" class="collapse navbar-collapse nano">
  	<div class="nano-content">
  		{% include 'menu.html.twig' %}
  	</div>
  </aside>

	<div id="main">
		<div class="content">
			{% block main %}

			  <section>
			    <div class="section-header">
			      <ol class="breadcrumb">
			        {% block breadcrumb %}{% endblock %}
			      </ol>
			    </div>
			    <div class="section-body">
			      {% block sectionBody %}{% endblock %}
			    </div>
			  </section>

			{% endblock %}
	  </div>
  </div>

</div>


	<script src="{{ asset('bower_components/jquery/dist/jquery.min.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bower_components/bootstrap/dist/js/bootstrap.min.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bower_components/bootstrap-material-design/dist/js/material.min.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bower_components/bootstrap-material-design/dist/js/ripples.min.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bower_components/metisMenu/dist/metisMenu.min.js') }}" type="text/javascript"></script>
	<script src="{{ asset('bower_components/nanoscroller/bin/javascripts/jquery.nanoscroller.min.js') }}" type="text/javascript"></script>
	<script src="{{ asset('js/laFuente/PrestamosApp.js') }}" type="text/javascript"></script>
	{% block javascripts %}{% endblock %}
</body>
</html>
{% endblock %}
