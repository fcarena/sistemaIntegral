{% extends "LaFuenteCertificadoBundle::default.html.twig" %}

{% block main %}
    <h1>TipoCertificado</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipoCertificado.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ tipoCertificado.nombre }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipocertificado_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tipocertificado_edit', { 'id': tipoCertificado.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
